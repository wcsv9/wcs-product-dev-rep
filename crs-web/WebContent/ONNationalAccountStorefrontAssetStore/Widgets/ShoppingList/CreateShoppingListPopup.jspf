<c:set var="dialogOptions" value='{"close_button_selector": "a[id$=createShoppingListPopup_close]", "primary_button_selector": "a[id$=createShoppingListPopup_save]", "secondary_button_selector": "a[id$=createShoppingListPopup_cancel]"}' />
<div id="<c:out value="${param.parentPage}"/>createShoppingListPopup" data-widget-type="wc.WCDialog" data-widget-options="${fn:escapeXml(dialogOptions)}" title="<fmt:message bundle="${storeText}" key='DIALOG_TITLE_CREATE_WISHLIST' />" style="display:none;">
    
    <!-- BEGIN CreateShoppingListPopup.jspf (Store) -->
	<div class="widget_site_popup">
		<div class="top">
			<div class="left_border"></div>
			<div class="middle"></div>
			<div class="right_border"></div>
		</div>
		<div class="clear_float"></div>
		<div class="middle">
			<div class="content_left_border">
				<div class="content_right_border">
					<div class="content">
						<div class="header">
							<span><fmt:message bundle="${storeText}" key="SL_CREATE_NEW_SHOPPING_LIST" /></span>
							<a role="button" id="${param.parentPage}createShoppingListPopup_close" href="javascript:void(0);" aria-label="<fmt:message bundle="${storeText}" key="SL_CLOSE" />" title="<fmt:message bundle="${storeText}" key="SL_CLOSE" />"
							class="close tlignore" tabindex="4" title="<fmt:message bundle="${storeText}" key="SL_CLOSE" />"></a>
							<div class="clear_float"></div>
						</div>
						<div class="body">
							<div id="${param.parentPage}shoppingListErrorMessageArea" style="display:none">
								<img class="error_icon" src="${jspStoreImgDir}images/colors/color1/error_icon.png" alt=""/>
								<p id="${param.parentPage}shoppingListErrorMessageText" class="textinlinewithicon"></p>
								<div class="clear_float"></div>
							</div>
							<div class="item_spacer_10px"></div>
							<p><fmt:message bundle="${storeText}" key="SL_NAME_NEW_LIST" /></p>
							<form>
								<span class="spanacce">
									<label for="${param.parentPage}newListName"><fmt:message bundle="${storeText}" key="SL_NAME_NEW_LIST" /></label>
								</span>
								<input id="<c:out value="${param.parentPage}"/>newListName" type="text" size="35" maxlength="64" value="" onkeypress='if(event.keyCode==13){ shoppingListJS<c:out value="${param.parentPage}"/>.create(); Utils.stopEvent(event);}' tabindex="1"/>
							</form>
							<div class="item_spacer_10px"></div>
						</div>
						<div class="footer">
							<div class="button_container">
								<a id="<c:out value="${param.parentPage}"/>createShoppingListPopup_save" href="javascript: shoppingListJS<c:out value="${param.parentPage}"/>.create();" class="button_primary tlignore" tabindex="2">
									<div class="left_border"></div>
									<span id="<c:out value="${param.parentPage}"/>CreateShoppingListPopup_div_create_save" class="button_text"><fmt:message bundle="${storeText}" key="SAVE" /></span>
									<div class="right_border"></div>
								</a>
								<a id="<c:out value="${param.parentPage}"/>createShoppingListPopup_cancel" href="javascript:void(0);"
                                <c:if test="${param.parentPage == 'QuickInfo'}">
                                    onclick="QuickInfoJS.showDetails('<c:out value="${param.productId}"/>');" 	
                                </c:if>
								class="button_secondary tlignore" tabindex="3">
									<div class="left_border"></div>
									<span class="button_text"><fmt:message bundle="${storeText}" key="CANCEL" /></span>
									<div class="right_border"></div>
								</a>
							</div>
						</div>
						
					</div>
				</div>
			</div>
		</div>
		<div class="clear_float"></div>
		<div class="bottom">
			<div class="left_border"></div>
			<div class="middle"></div>
			<div class="right_border"></div>
		</div>
		<div class="clear_float"></div>
	</div>
    <!-- END CreateShoppingListPopup.jspf (Store) -->
</div>