<?xml version="1.0" encoding="UTF-8"?>
<web-ext
	xmlns="http://websphere.ibm.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://websphere.ibm.com/xml/ns/javaee http://websphere.ibm.com/xml/ns/javaee/ibm-web-ext_1_0.xsd"
	version="1.0">

	<default-error-page uri="error.jsp"/>
	<file-serving-attribute name="file.serving.patterns.deny"
		value="WEB-INF/* META-INF/*" />
	<jsp-attribute name="keepgenerated" value="true" />
	<jsp-attribute name="trackDependencies" value="true" />
	<jsp-attribute name="useCDataTrim" value="true" />
	<jsp-attribute name="reloadEnabled" value="true"/>
	<jsp-attribute name="reloadInterval" value="10"/>
	<reload-interval value="0"></reload-interval>
	<context-root uri="/wcs" />
	<enable-directory-browsing value="false"/>
	<enable-file-serving value="true"/>
	<enable-reloading value="true"/>
	<enable-serving-servlets-by-class-name value="false" />

</web-ext>
