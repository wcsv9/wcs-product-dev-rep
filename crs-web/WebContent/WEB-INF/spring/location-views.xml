<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">  
  <bean id="AjaxStoreLocatorDisplayView" class="com.ibm.commerce.store.mvc.view.spring.StoreInternalResourceView"> 
    <property name="url" value="/StoreLocatorArea/StoreLocatorDisplay.jsp"/>  
    <property name="credentialsAccepted" value="P"/> 
    <property name="generic" value="1"/>
  </bean>  
  <bean id="AjaxProvinceSelectionDisplayView" class="com.ibm.commerce.store.mvc.view.spring.StoreInternalResourceView"> 
    <property name="url" value="/Snippets/StoreLocator/ProvinceSelectionDisplay.jsp"/>  
    <property name="credentialsAccepted" value="P"/> 
    <property name="generic" value="1"/>
  </bean>  
  <bean id="AjaxStoreLocatorResultsView" class="com.ibm.commerce.store.mvc.view.spring.StoreInternalResourceView"> 
    <property name="url" value="/Snippets/StoreLocator/StoreLocatorResults.jsp"/> 
    <property name="generic" value="1"/>
  </bean>  
  <bean id="AjaxCitySelectionDisplayView" class="com.ibm.commerce.store.mvc.view.spring.StoreInternalResourceView"> 
    <property name="url" value="/Snippets/StoreLocator/CitySelectionDisplay.jsp"/>  
    <property name="credentialsAccepted" value="P"/> 
    <property name="generic" value="1"/>
  </bean> 
</beans>