<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">  
  <bean id="WishListDisplayView" class="com.ibm.commerce.store.mvc.view.spring.StoreInternalResourceView"> 
    <property name="url" value="/UserArea/ServiceSection/InterestItemListSubsection/WishListDisplay.jsp"/>  
    <property name="https" value="1"/>  
    <property name="authenticate" value="1"/> 
  </bean>  
  <bean id="InterestItemDisplay" class="com.ibm.commerce.store.mvc.view.spring.StoreInternalResourceView"> 
    <property name="url" value="/UserArea/ServiceSection/InterestItemListSubsection/SharedWishListDisplay.jsp"/> 
  </bean> 
  <bean id="WishListResultDisplayView" class="com.ibm.commerce.store.mvc.view.spring.StoreInternalResourceView"> 
    <property name="url" value="/UserArea/ServiceSection/InterestItemListSubsection/WishListResultDisplay.jsp"/> 
  </bean>  
  <bean id="MultipleWishListSelectionView" class="com.ibm.commerce.store.mvc.view.spring.StoreInternalResourceView"> 
    <property name="url" value="/UserArea/ServiceSection/InterestItemListSubsection/MultipleWishListController.jsp"/>  
    <property name="authenticate" value="1"/>  
    <property name="https" value="1"/> 
  </bean> 
  <bean id="InterestItemListMessageView" class="com.ibm.commerce.store.mvc.view.spring.StoreInternalResourceView"> 
    <property name="url" value="/EmailTemplates/User/WishlistCreateNotify.jsp"/> 
  </bean>
  <bean id="InterestItemListMessageView-Silverpop" class="com.ibm.commerce.store.mvc.view.spring.StoreInternalResourceView"> 
    <property name="url" value="/EmailTemplates/User/SilverpopWishlistCreateNotify.jsp"/>
    <property name="skipAccessControlCheck" value="1"/> 
  </bean>
  <bean id="AllWishListsInProductDetailView" class="com.ibm.commerce.store.mvc.view.spring.StoreInternalResourceView"> 
    <property name="url" value="/Snippets/MultipleWishList/AddToWishListPanelRefreshArea.jsp"/>  
    <property name="authenticate" value="1"/> 
  </bean>
  <bean id="SharedWishListView" class="com.ibm.commerce.store.mvc.view.spring.StoreInternalResourceView"> 
    <property name="url" value="/UserArea/ServiceSection/InterestItemListSubsection/SharedWishListDisplay.jsp"/> 
  </bean>  
  <bean id="InterestItemDisplayView" class="com.ibm.commerce.store.mvc.view.spring.StoreInternalResourceView"> 
    <property name="url" value="/UserArea/ServiceSection/InterestItemListSubsection/SharedWishListDisplay.jsp"/> 
  </bean>             
</beans>