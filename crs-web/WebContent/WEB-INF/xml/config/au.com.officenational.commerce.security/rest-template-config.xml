<_config:rest-action-config xmlns:_config="http://www.ibm.com/xmlns/prod/commerce/foundation/config" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.ibm.com/xmlns/prod/commerce/foundation/config ../../xsd/rest-action-config.xsd ">
	<resource path="{serverHost}/{contextPath}/store/{storeId}/userApproval" name="onLogonWPHandler">
	 <serverParam name="serverHost">$WCParam.serverHost</serverParam>
	 <serverParam name="contextPath">$WCParam.contextPath</serverParam>
	 <serverParam name="storeId">$WCParam.storeId</serverParam>
	 	<method name="onLogonWP" httpMethod="GET" path="onlogonwp?langId={langId}&amp;catalogId={catalogId}&amp;userId={userId}&amp;logonId={logonId}&amp;logonPassword={logonPassword}&amp;reLogonURL={reLogonURL}&amp;URL={URL}&amp;forwardCommand={forwardCommand}&amp;forwardParameter={forwardParameter}">
	 		<var name="langId">$WCParam.langId</var>
	 		<var name="catalogId">$WCParam.catalogId</var>
	 		<var name="userId">$WCParam.userId</var>
	 		<var name="logonId">$WCParam.logonId</var>
			<var name="logonPassword">$WCParam.logonPassword</var>
			<var name="reLogonURL">$WCParam.reLogonURL</var>
			<var name="URL">$WCParam.URL</var>
			<var name="forwardCommand">$WCParam.forwardCommand</var>
			<var name="forwardParameter">$WCParam.forwardParameter</var>
    	</method>  
		
			
	</resource>
</_config:rest-action-config>