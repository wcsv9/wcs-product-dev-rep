<c:if test="${!pageloaded_shopListCreateSuccessJSPF}">
	<c:set var="dialogOptions" value='{"close_button_selector": "a[id$=shoppingListCreateSuccessPopup_close]", "primary_button_selector": "a[id$=shoppingListCreateSuccessPopup_continue_shopping]"}' /> 
    <div
		id="<c:out value="${param.parentPage}"/>shoppingListCreateSuccessPopup"
		data-widget-type="wc.WCDialog"
		title="<wcst:message key='LIST_CREATED' bundle='${widgetText}' />"
		style="display: none;">
		<div class="widget_site_popup">
			<div class="top">
				<div class="left_border"></div>
				<div class="middle"></div>
				<div class="right_border"></div>
			</div>
			<div class="clear_float"></div>
			<div class="middle">
				<div class="content_left_border">
					<div class="content_right_border">
						<div class="content">
							<a role="button"
								id="<c:out value="${param.parentPage}"/>shoppingListCreateSuccessPopup_close"
								href="javascript:void(0);"
								aria-label="<wcst:message key="SL_CLOSE" bundle="${widgetText}"/>"
								title="<wcst:message key="SL_CLOSE" bundle="${widgetText}"/>"
								<c:choose>
								<c:when test="${param.parentPage == 'QuickInfo'}">
									onclick="QuickInfoJS.showDetails('<c:out value="${param.productId}"/>'); javascript: document.getElementById('<c:out value="${param.parentPage}"/>addToShoppingList').focus();"	
								</c:when>
								<c:otherwise>
									onclick="shoppingListJS.hideMessageDialog(); javascript: document.getElementById('<c:out value="${param.parentPage}"/>addToShoppingList').focus();"
								</c:otherwise>
							</c:choose>
								class="close tlignore"
								title="<wcst:message key="SL_CLOSE" bundle="${widgetText}" />"></a>
							<div class="body">
								<div class="item_spacer_10px"></div>
								<div class="item_spacer_10px"></div>
								<div class="item_spacer_10px"></div>
								<div class="item_spacer_10px"></div>
								<img class="error_icon"
									src="<c:out value='${jspStoreImgDir}${env_vfileColor}'/>success_icon.png"
									alt="" />
								<p
									id="<c:out value="${param.parentPage}"/>successMessageAreaText"
									class="textinlinewithicon">
									<wcst:message key="LIST_CREATED" bundle="${widgetText}" />
								</p>
								<div class="clear_float"></div>
								<div class="item_spacer_10px"></div>
								<div class="item_spacer_10px"></div>
								<div class="item_spacer_10px"></div>
								<div class="item_spacer_10px"></div>
							</div>
							<div class="footer">
								<div class="button_container">
									<a
										id="<c:out value="${param.parentPage}"/>shoppingListCreateSuccessPopup_continue_shopping"
										href="javascript:void(0);"
										<c:choose>
										<c:when test="${param.parentPage == 'QuickInfo'}">
											onclick="QuickInfoJS.showDetails('<c:out value="${param.productId}"/>'); javascript: document.getElementById('<c:out value="${param.parentPage}"/>addToShoppingList').focus();" 	
										</c:when>
										<c:otherwise>
											onclick="shoppingListJS.hideMessageDialog(); javascript: document.getElementById('<c:out value="${param.parentPage}"/>addToShoppingList').focus();" 
										</c:otherwise>
									</c:choose>
										class="button_primary tlignore">
										<div class="left_border"></div>
										<div class="button_text">
											<wcst:message key="SL_CONTINUE_SHOPPING"
												bundle="${widgetText}" />
										</div>
										<div class="right_border"></div>
									</a>
									<div class="clear_float"></div>
								</div>
								<div class="clear_float"></div>
							</div>
							<div class="clear_float"></div>
						</div>
					</div>
				</div>
			</div>
			<div class="clear_float"></div>
			<div class="bottom">
				<div class="left_border"></div>
				<div class="middle"></div>
				<div class="right_border"></div>
			</div>
			<div class="clear_float"></div>
		</div>
	</div>
	<c:set var="pageloaded_shopListCreateSuccessJSPF" value="true"
		scope="request" />
</c:if>