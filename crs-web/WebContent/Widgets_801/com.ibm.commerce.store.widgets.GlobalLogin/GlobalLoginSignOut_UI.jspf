<a class="panelLinkSelected Header_GlobalLogin_signOutQuickLink_CSR" id="${widgetId}_signOutQuickLink" aria-haspopup="true" aria-label="<wcst:message bundle='${widgetText}' key='GLOBALLOGIN_SIGN_OUT_PANEL'/>"
  href="javaScript:GlobalLoginJS.displayPanel('${widgetId}');" onmouseover="javaScript:GlobalLoginJS.hideGLPanel(false);">
    <img class="userProfileIcon" alt="<wcst:message bundle="${widgetText}" key="ACCE_USER_PROFILE_ICON"/>" src="<c:out value='${jspStoreImgDir}'/>images/user-profile-icon.png" />
    <span id="${widgetId}_signOutQuickLinkUser"></span>
    <img id="${widgetId}_desktopExpandArrow" src="<c:out value='${jspStoreImgDir}'/>images/user-profile-arrow.png" alt=""/>
</a>
<div wcType="RefreshArea" declareFunction="declareSignOutRefreshArea('<c:out value="${widgetId}"/>')" class="GlobalLoginWidgetAlt" widgetId="${widgetId}" id="${widgetId}" refreshurl="<c:out value='${GlobalLoginDisplayURL}'/>"role="region" aria-label="<wcst:message bundle="${widgetText}" key="GLOBALLOGIN_SIGN_OUT_PANEL"/>" ariaMessage="<wcst:message key="ACCE_STATUS_GLOBALLOGIN_UPDATED" bundle="${widgetText}" />"
    data-toggle-control="${widgetId}_signOutQuickLink" >
</div>
