<%-- Order Detail - order info area --%>
<c:set var="pageName" value=""/>
<c:choose>
	<c:when test="${order.orderStatus == 'I'}">
		<c:set var="pageName" value="Recurring Orders"/>
	</c:when>
	<c:otherwise>
		<c:set var="pageName" value="Order History"/>
	</c:otherwise>
</c:choose>
<input type="hidden" id="pageName" value="${pageName }"/>
<div class="row headInsideMenu">
	<div class="col1">&nbsp;</div>
	<div class="col10 userIconLine">
		<c:if test="${pageName == 'Recurring Orders'}">
			<div class="recurringOrderUserIcon"></div>
		</c:if>
		<c:if test="${pageName == 'Order History'}">
			<div class="orderHistoryPageUserIcon"></div>
		</c:if>
		<hr />
	</div>
	<div class="col1">&nbsp;</div>
	<div class="col12">
		<p>${pageName}</p>
	</div>
	<div class="col12 links">
		<div class="normalMenu" id="accDetailMenu">
			<%out.flush();%>
			<c:import url="${env_jspStoreDir}Common/MyAccountPageURLs.jsp" />
			<%out.flush();%>
		</div>
	</div>
</div>


