	<input type="hidden" id="orderDetailItemManufacturer_<c:out value="${orderItemInfo.partNumber }"/>" value="${orderItemInfo.manufacturer }"/>
	<input type="hidden" id="orderDetailItemName_<c:out value="${orderItemInfo.partNumber }"/>" value="${orderItemInfo.name }"/>
	
	<!-- Get Category name for GTM -->
