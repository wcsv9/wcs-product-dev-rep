	<input type="hidden" name="qtyCounter" id="qtyCounter" value="<c:out value='${qtyCounterLength + 1}'/>">
	
	<!-- Get Category for GTM -->
	<%-- <c:forEach var="curCatEntryView" items="${catEntry.catalogEntryView}">
		<c:set var="splt" value="${fn:split(curCatEntryView.parentCatalogGroupID[0],'_')}"/>
		<wcf:rest var="category"
			url="${searchHostNamePath}${searchContextPath}/store/${WCParam.storeId}/categoryview/byId/${splt[1] }">
			<wcf:param name="langId" value="${langId}" />
			<wcf:param name="currency" value="${env_currencyCode}" />
			<wcf:param name="responseFormat" value="json" />
			<wcf:param name="catalogId" value="${WCParam.catalogId}" />
		</wcf:rest>
		<input type="hidden" value='<c:out value="${category.catalogGroupView[0].longDescription }"/>' id='favouriteItemCategory_<c:out value="${curCatEntryView.partNumber}"/>'/>
	</c:forEach> --%>
	<c:forEach var="orderItem" items="${order.orderItem}" varStatus="istat">
	
		<c:set var="productId" value="${orderItem.productId }"/>
		
		<c:remove var="catEntryView"/>
		<c:forEach var="curCatEntryView" items="${catEntry.catalogEntryView}">
			<c:if test="${curCatEntryView.uniqueID == orderItem.productId}">
				<c:set var="catEntryView" value="${curCatEntryView}" />
			</c:if>
		</c:forEach>
		
		<wcf:rest var="categoryTree" url="store/{storeId}/customCatalogHandler/@getCategoryTree" scope="request" cached="true">
			<wcf:var name="storeId" value="${storeId}" encode="true"/>
			<wcf:param name="catalogId" value="${WCParam.catalogId}"/>
      		<wcf:param name="productId" value="${productId}"/>
		</wcf:rest>

		<c:set var="rowCount" value="${rowCount+1}"/>
		<c:set var="count" value="${istat.count}"/>
		
		<c:set var="cat" value="${WCParam.cat}"/>
		<c:set var="checkFilter" value="false"/>
		<c:if test="${fn:contains(cat, 'And')}">
		   <c:set var="cat" value="${fn:replace(cat,'And', '&')}" />
		</c:if>

		<c:choose>
			<c:when test="${cat ne categoryTree.catagoryName && !empty cat}">
			</c:when>
			<c:when test="${empty cat}">
				<c:set var="type" value="${fn:toLowerCase(catEntryView.catalogEntryTypeCode)}" />
				<c:set var="type" value="${fn:replace(type,'bean','')}" />
				<c:set var="AddPDK" value="false" />
				<c:set var="AddDK" value="false" />
				<c:set var="ConfigXML" value="" />
				<c:choose>
					<c:when test="${type eq 'dynamickit'}">
						<wcf:rest var="itemConfiguration" url="store/{storeId}/order">
							<wcf:var name="storeId" value="${storeId}" encode="true"/>
							<wcf:param name="q" value="findConfigurationByOrderItemId"/>
							<wcf:param name="orderItemId" value="${orderItem.orderItemId}"/>
						</wcf:rest>
						<c:if test="${!empty itemConfiguration.Order && !empty itemConfiguration.Order[0].orderItem && !empty itemConfiguration.Order[0].orderItem[0].orderItemConfiguration}">
							<c:set var="ConfigXML" value="${itemConfiguration.Order[0].orderItem[0].orderItemConfiguration}" />
							<c:set var="ConfigXML">
							    <wcf:json object="${ConfigXML}" />
							</c:set>
						</c:if>
						<c:set var="AddDK" value="true" />
					</c:when>
					<c:when test="${type eq 'preddynakit'}">
						<c:set var="AddPDK" value="true" />
					</c:when>
					<c:otherwise>
						<c:set var="AddDK" value="false" />
						<c:set var="AddPDK" value="false" />
					</c:otherwise>
				</c:choose>
				
				<wcf:useBean var="inputAttribute" classname="java.util.HashMap"/>
					<wcf:set key="name" value="catEntryId_${rowCount}" target="${inputAttribute}" />
					<wcf:set key="value" value="${catEntryView.uniqueID}" target="${inputAttribute}" />
				<wcf:set target="${tableFormHiddenInputs}" value="${inputAttribute}" />
				<c:remove var="inputAttribute"/>
				
				<wcf:useBean var="inputAttribute" classname="java.util.HashMap"/>
					<wcf:set key="id" value="ProductInfoName_${catEntryView.uniqueID}" target="${inputAttribute}" />
					<wcf:set key="name" value="ProductInfoName_${catEntryView.uniqueID}" target="${inputAttribute}" />
					<wcf:set key="value" value="${catEntryView.name}" target="${inputAttribute}" />
				<wcf:set target="${tableFormHiddenInputs}" value="${inputAttribute}" />
				<c:remove var="inputAttribute"/>
		
				<wcf:useBean var="inputAttribute" classname="java.util.HashMap"/>
					<wcf:set key="id" value="ProductInfoImage_${catEntryView.uniqueID}" target="${inputAttribute}" />
					<wcf:set key="name" value="ProductInfoImage_${catEntryView.uniqueID}" target="${inputAttribute}" />
					<wcf:set key="value" value="${fn:escapeXml(catEntryView.thumbnail)}" target="${inputAttribute}" />
				<wcf:set target="${tableFormHiddenInputs}" value="${inputAttribute}" />
				<c:remove var="inputAttribute"/>
		
				<wcf:useBean var="rCells" classname="java.util.ArrayList"/>
					<%-- table row column 1 --%>
					<c:remove var="thumbnail"/>
					<c:if test="${!empty catEntryView.thumbnail}">
						<c:choose>
							<c:when test="${(fn:startsWith(catEntryView.thumbnail, 'http://') || fn:startsWith(catEntryView.thumbnail, 'https://'))}">
								<wcst:resolveContentURL var="thumbnail" url="${catEntryView.thumbnail}"/>
							</c:when>
							<c:when test="${fn:startsWith(catEntryView.thumbnail, '/store/0/storeAsset')}">
								<c:set var="thumbnail" value="${storeContextPath}${catEntryView.thumbnail}" />
							</c:when>
							<c:otherwise>
								<c:set var="thumbnail" value="${catEntryView.thumbnail}" />
							</c:otherwise>
						</c:choose>
					</c:if>
					
					<wcf:useBean var="rCell" classname="java.util.HashMap"/>
						<wcf:set key="div1Class" value="col2 previewImage" target="${rCell}" />
						<wcf:set key="div2Class" value="cell" target="${rCell}" />
