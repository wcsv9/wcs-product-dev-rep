	<div id="requisition_list_table_paging" class="requisitionListBorder_top requisitionEditListPage">
		<span>
			<wcf:rest var="categoryHierarchy" url="${searchHostNamePath}${searchContextPath}/store/${WCParam.storeId}/categoryview/@top" >
				<wcf:param name="langId" value="${langId}"/>
				<wcf:param name="responseFormat" value="json"/>		
				<wcf:param name="catalogId" value="${WCParam.catalogId}"/>
				<wcf:param name="depthAndLimit" value="${depthAndLimit}"/>
				<c:forEach var="contractId" items="${env_activeContractIds}">
					<wcf:param name="contractId" value="${contractId}"/>
				</c:forEach>
			</wcf:rest>
			<c:forEach var="department" items="${categoryHierarchy.catalogGroupView}" varStatus="status">
					<c:set var="categoryName" value="${department.identifier}"/>
			</c:forEach>
			<b>Category</b>&nbsp;
			<select name="selCategory" id="selCategory" onchange="ReqListItemsJS.catFilter(this.options[selectedIndex].text);">
				<option value="0">ALL</option>
				<c:forEach var="department" items="${categoryHierarchy.catalogGroupView}" varStatus="status">
					<c:set var="categoryName" value="${department.identifier}"/>
					
					<c:if test="${(categoryName eq 'CANTEEN, JANITORIAL AND OHS' || categoryName eq 'Canteen, Janitorial and OHS')}">
						<c:set var="categoryName" value="CANTEEN"/>
					</c:if>
		
					<c:if test="${(categoryName eq 'WAREHOUSE AND PACKAGING' || categoryName eq 'Warehouse and Packaging')}">
						<c:set var="categoryName" value="WAREHOUSE"/>
					</c:if>
					
					<c:if test="${(categoryName eq 'PRINT AND PROMOTION' || categoryName eq 'Print and Promotion')}">
						<c:set var="categoryName" value="PRINT"/>
					</c:if>
					
					<c:if test="${(categoryName eq 'INKS, TONERS AND RIBBONS' || categoryName eq 'Ink, Toners And Ribbons')}">
						<c:set var="categoryName" value="INK AND TONERS"/>
					</c:if>
					
					<c:if test="${(categoryName eq 'PAPER SUPPLIES' || categoryName eq 'Paper Supplies')}">
						<c:set var="categoryName" value="PAPER"/>
					</c:if>
					
					<c:set var="cat" value="${WCParam.cat}"/>
					
					<c:if test="${fn:contains(cat, 'And')}">
					   <c:set var="cat" value="${fn:replace(cat,'And', '&')}" />
					</c:if>
					
					<c:choose>
						<c:when test="${cat eq categoryName}">
							<option value="<c:out value='${department.uniqueID}'/>" selected="selected"> <c:out value="${categoryName}" escapeXml="false"/> </option>
						</c:when>
						<c:when test="${filterTopCategoryId ne department.uniqueID}">
							<option value="<c:out value='${department.uniqueID}'/>"> <c:out value="${categoryName}" escapeXml="false"/> </option>
						</c:when>
					</c:choose>
			    </c:forEach>
			</select>
		</span>
	</div>

