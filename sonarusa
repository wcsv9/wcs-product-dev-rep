
 node ('master') {
 stage('SCM') {
    git 'https://github.com/wcsv9/wcs-product-dev-rep.git'
  }
   stage('SonarQube analysis') {
   
    // requires SonarQube Scanner 2.8+
    def scannerHome = tool 'sonar';
    withSonarQubeEnv('sonar') {
      sh "${scannerHome}/bin/sonar-scanner scan -Dsonar.host.url=http://192.168.7.187:9000 -Dproject.settings=/var/lib/jenkins/workspace/sonar/sona-usa.properties"
    
  }
}
}
